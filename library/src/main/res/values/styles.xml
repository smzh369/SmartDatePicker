<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="SmartDatePicker">
        <attr name="format" format="flags">
            <flag name="yyyy-MM-dd" value="yyyy-MM-dd" />
            <flag name="yyyy-MM" value="yyyy-MM" />
        </attr>
        <attr name="style" format="enum">
            <enum name="wheel_light" value="3"/>
            <enum name="wheel_dark" value="2"/>
            <enum name="calender_light" value="5"/>
            <enum name="calender_dark" value="4"/>
        </attr>
    </declare-styleable>

</resources>